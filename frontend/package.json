{
  "name": "schier.dev",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "rm -rf ./static/* && parcel watch --out-dir ./static --public-url /static/ index.*.js index.*.css",
    "build": "rm -rf ./static/* && parcel build --no-source-maps --out-dir ./static index.*.js index.*.css"
  },
  "staticFiles": {
    "staticPath": [
      {
        "staticPath": "images",
        "staticOutDir": "images"
      },
      {
        "staticPath": "dictionaries",
        "staticOutDir": "dictionaries"
      },
      {
        "staticPath": "favicon",
        "staticOutDir": "favicon"
      }
    ]
  },
  "browserslist": [
    "last 1 Chrome versions"
  ],
  "devDependencies": {
    "autoprefixer": "^9.8.5",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-static-files-copy": "^2.4.3",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.3",
    "tailwindcss": "^1.6.0"
  },
  "dependencies": {
    "codemirror": "^5.56.0",
    "codemirror-typo": "^1.0.1"
  }
}
